{
    "name": "govtnz/progresslogentry",
    "description": "Module for logging progress of a task. Can also be derived from to create tasks and track their progress. Requires SilverStripe >= 3.0.",
    "homepage": "https://github.com/govtnz/progresslogentry",
    "type": "silverstripe-module",
 	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/govtnz/progresslogentry"
		}
	],
	"require": {
			"php": ">=5.3",
			"silverstripe/framework": ">=3.0"
 	},
	"authors": [
 		{
 			"name": "wakes",
 			"email": "wakes@github.com",
 			"homepage": "https://github.com/wakes/",
 			"role": "Developer"
 		}
 	],
 	"support": {
 		"issues": "https://github.com/govtnz/progresslogentry/issues"
 	},
 	"autoload": {
 		"classmap": [
 			"src/"
 		]
 	},
 	"extra": {
 		"installer-name": "progresslogentry"
 	}
}
